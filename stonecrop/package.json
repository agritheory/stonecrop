	"devDependencies": {
		"@stonecrop/aform": "workspace:*",
		"@stonecrop/atable": "workspace:*",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"@vitejs/plugin-vue": "^5.0.4",
		"eslint": "^8.40.0",
